<%= form_page_header('Media File') %>

<%= error_messages_for @media_file %>

<%= form_for @media_file, multipart: true do |f| %>
  <%= f.label :source %>
  <%= f.file_field :source  %>

  <p>
    <%= f.submit 'Upload Media File', class: 'button submit-button' %> or <%= link_to 'Cancel', media_files_path  %>
  </p>
<% end %>